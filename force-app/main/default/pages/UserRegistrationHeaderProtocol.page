<apex:page controller="UserRegistrationHeaderCtrl" showheader="false" sidebar="false" standardStylesheets="false">
    <head>
        <title>ADGM</title>
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>
    <body>
        <style>
            body .bEditBlock .pbBody .pbSubheader, 
            body .bWizardBlock .pbBody .pbSubheader {
                background: #61bae4; /* secondary color */
                color: #fff !important;
            }
            .detailPage .bPageBlock, .editPage .bPageBlock {
                border-top-width: 4px !important;
            }
            #portalheader{
                background: #FFFFFF; /* primary color */
            }
            .navbar-collapse {
                overflow-x: visible;
                padding-right: 15px;
                padding-left: 15px;
                border-top: 1px solid transparent;
                box-shadow: inset 0 1px 0 rgba(255,255,255,.1);
                -webkit-overflow-scrolling: touch;
            }
            
        </style>
        <div id="portalwrapper">
            <div id="portalheader" style="border-color:#ac975e !important;">
                <nav class="navbar menu-header navbar-hexabpm">
                    <div class="container-fluid">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 " id="nav-div">
                            <div class="menu-header">
                                <div class="navbar-brand">
                                    <apex:image id="primarylogo" value="{!PrimaryLogoURL}"  height="50px"/>
                                </div>
                                    
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </body>
</apex:page>